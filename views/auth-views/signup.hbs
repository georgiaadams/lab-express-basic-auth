<form action="/auth/signup" method="POST">
<h2>SIGN UP</h2>

<label for="">Username</label>
<input type="text" name="username">

<br>

<label for="">Password</label>
<input type="password" name="password">

<br>

<button type="submit">Create Account</button>

</form>

{{#if errorMessage}}
<div>
    {{errorMessage}}
</div>

{{/if}}

